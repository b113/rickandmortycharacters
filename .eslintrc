{
    "plugins" : ["react"],
    "extends" : "airbnb",
    "globals": {
        "document": "true",
    },
    "rules" : {
        "no-console": "off",
        "react/jsx-filename-extension" : [
            1,
            {
                "extensions" : [".js", ".jsx"]
            }
        ]
    }
}